{
  "name": "Sheriyans-projects",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "generate": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy"
  },
  "devDependencies": {
    "@types/node": "^22.15.13",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0",
    "tsx": "^4.19.4",
    "turbo": "^2.5.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^6.7.0"
  }
}
